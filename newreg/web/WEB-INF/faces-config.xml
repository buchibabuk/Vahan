        <action-listener>org.primefaces.application.DialogActionListener</action-listener>
        <navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
        <view-handler>org.primefaces.application.DialogViewHandler</view-handler>
    </application>

    <!--for loading properties file globally-->
    <application>
        <resource-bundle>
            <base-name>nic.vahan.resources.label_names</base-name>
            <var>label</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>nic.vahan.resources.permit_lable_name</base-name>
            <var>pmtLabel</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>nic.vahan.resources.constant_values</base-name>
            <var>constant</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>nic.vahan.resources.eAppInstructionMessages</base-name>
            <var>msg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>nic.vahan.resources.imageResource</base-name>
            <var>imageName</var>
        </resource-bundle>
        
    </application>

    <render-kit>
        <renderer>
            <component-family>javax.faces.Form</component-family>
            <renderer-type>javax.faces.Form</renderer-type>
            <renderer-class>nic.vahan.common.jsf.utils.MyFormRenderer</renderer-class>
        </renderer>
    </render-kit>
    <lifecycle>
        <phase-listener>nic.vahan.common.jsf.utils.MultipleSubmitFormPreventionPhaseListener</phase-listener>
    </lifecycle>
    <managed-bean>
        <managed-bean-name>visitedTokenMap</managed-bean-name>
        <managed-bean-class>java.util.HashMap</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <application>   
        <!-- Application is started -->
        <system-event-listener>
            <system-event-listener-class>
                nic.vahan.common.jsf.utils.JSFAppListener
            </system-event-listener-class>
            <system-event-class>
                javax.faces.event.PostConstructApplicationEvent
            </system-event-class>    					
        </system-event-listener> 	 
 
        <!-- Before Application is shut down -->
        <system-event-listener>
            <system-event-listener-class>
                nic.vahan.common.jsf.utils.JSFAppListener
            </system-event-listener-class>
            <system-event-class>
                javax.faces.event.PreDestroyApplicationEvent
            </system-event-class>    					
        </system-event-listener> 
        	
    </application>
    <!--    add language -->
    <application>
        <locale-config>
            <!-- default locale and supported locales -->
            <default-locale>en</default-locale>
            <supported-locale>hi</supported-locale>
                     
        </locale-config>
        <!-- resource bundle configuration -->
        <resource-bundle>
            <!-- properties file base name with full package -->
            <base-name>nic.vahan.language.resources.messages</base-name>  
                                 
            <var>language</var>
        </resource-bundle>
      
    </application>
    
